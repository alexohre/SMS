<div id="<%= dom_id event %>">
	<p>
		<strong>Name:</strong>
		<%= event.name %>
	</p>
	<p>
		<strong>Description:</strong>
		<%= event.description %>
	</p>
	<p>
		<strong>Start date:</strong>
		<%= event.start_date %>
	</p>
	<p>
		<strong>Start time:</strong>
		<%= event.start_time %>
	</p>
	<p>
		<strong>Duration:</strong>
		<%= event.duration %>
	</p>
	<p>
		<strong>Photo:</strong>
		<%= image_tag(event.photo) %>
	</p>
</div>
